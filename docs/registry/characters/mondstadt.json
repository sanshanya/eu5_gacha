    {
      "code": "albedo",
      "name": {
        "first": "阿贝多",
        "last": "白垩"
      },
      "traits": {
        "origin": {
          "key": "gacha_albedo_origin_trait",
          "name": "白垩的素描本",
          "desc": "阿贝多：「这片大陆的矿脉走向与城防结构…很有意思。若允许我做些记录，我能把它们变成更稳固的城墙与更高效的工坊。」\n（他把纸笔放得很轻，仿佛连噪音都会干扰实验。）",
          "aura_country_modifier": "gacha_albedo_country_modifier",
          "modifier": {
            "gacha_country_aura": "yes",
            "global_construction_speed": 0.05,
            "global_defensive": 0.05
          }
        },
        "awakened": {
          "key": "gacha_albedo_awakened_trait",
          "name": "创生法·拟造阳华",
          "desc": "阿贝多：「临时的高台、可重复的支点、可控的坍塌。攻城并不只靠蛮力——也可以靠几何。」\n（你发现工匠们开始照着他的“配方”加固炮台与城门。）",
          "modifier": {
            "siege_ability": 0.05
          }
        },
        "transcended": {
          "key": "gacha_albedo_transcended_trait",
          "name": "诞生式·大地之潮",
          "desc": "阿贝多：「当压力积累到某个阈值，结构会以最省力的方式崩解。战争也是如此。」\n他把战场当作一张可推演的图纸：哪里会裂，哪里会倒，都能提前写在边注里。",
          "modifier": {
            "land_morale_recovery": 0.025,
            "land_unit_attrition": -0.025
          }
        },
        "c2": {
          "key": "gacha_albedo_c2_trait",
          "name": "显生之宙",
          "desc": "阿贝多：「记录并非为了回忆，而是为了复现。一次胜利若无法复现，就只是侥幸。」\n（你获得了一套“生灭计数”：每一次攻守都被归档，下一次会更快、更准。）",
          "aura_country_modifier": "gacha_albedo_c2_country_modifier",
          "modifier": {
            "gacha_country_aura": "yes",
            "global_construction_speed": 0.1,
            "global_defensive": 0.1
          }
        },
        "c4": {
          "key": "gacha_albedo_c4_trait",
          "name": "神性之陨",
          "desc": "阿贝多：「高度差会放大冲击。把优势做成地形，让它自己工作。」\n（从此，突袭与强攻不再只是赌运气，而像一场被计算好的坠击。）",
          "modifier": {
            "siege_ability": 0.1
          }
        },
        "c6": {
          "key": "gacha_albedo_c6_trait",
          "name": "无垢之土",
          "desc": "阿贝多：「结晶不是装饰，是“容错”。当变量过多时，先给体系一层壳。」\n你看见军阵外沿泛起沉稳的岩光：混乱被隔在外面，秩序留在阵内。",
          "modifier": {
            "land_morale_recovery": 0.05,
            "land_unit_attrition": -0.05
          }
        }
      },
      "constellations": {
        "c0": {
          "name": "白垩座·零命",
          "desc": "“白垩之子”并不追求神迹，只追求可验证的答案。随他而来的，是一套把城市、矿脉与军需都写成公式的工作方法。"
        },
        "c1": {
          "name": "伊甸之花",
          "desc": "每一朵“刹那之花”都像一次微型试验：让资源回流、让损耗可控。小收益被叠起来，最后变成稳定的优势。"
        },
        "c2": {
          "name": "显生之宙",
          "desc": "他开始给战争“计数”：胜利不是结果，而是过程的总和。只要计数不断累积，下一次爆发就更接近必然。"
        },
        "c3": {
          "name": "太阳之华",
          "desc": "阳华的构型被反复校正，工匠们甚至能照着图纸复刻。城防与工坊的效率，开始像日照一样稳定。"
        },
        "c4": {
          "name": "神性之陨",
          "desc": "当高台升起，重力就成了盟友。哪怕敌人披甲固守，也会在一轮轮坠击与震荡里露出裂缝。"
        },
        "c5": {
          "name": "冥古之潮",
          "desc": "更深的潮汐被唤醒：从地下到地表，整条补给链被重排。攻势不再断续，而是持续推进。"
        },
        "c6": {
          "name": "无垢之土",
          "desc": "最理想的体系不是完美，而是能承受错误。结晶的护持让你的军队在混战、围城与灾荒中仍能保持队形与纪律。"
        }
      },
