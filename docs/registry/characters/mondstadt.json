    {
      "code": "fischl",
      "name": {
        "first": "菲谢尔",
        "last": "幽夜"
      },
      "traits": {
        "origin": {
          "key": "gacha_fischl_origin_trait",
          "name": "幽夜的窥视",
          "desc": "菲谢尔：「此世的阴影中充满了虫豸的低语。但在本皇女的『断罪之眼』下，邻国的诡计如同白纸般苍白！没有什么能逃过幽夜的凝视！」\n奥兹：「小姐的意思是，我会负责侦查工作。这片大陆的间谍网似乎很薄弱，交给我去刺探情报吧。」",
          "aura_country_modifier": "gacha_fischl_country_modifier",
          "modifier": {
            "gacha_country_aura": "yes",
            "spy_network_construction": 0.4,
            "max_diplomats": 1
          }
        },
        "awakened": {
          "key": "gacha_fischl_awakened_trait",
          "name": "断罪雷影之弓",
          "desc": "菲谢尔：「降下裁决之时已到！令那轰鸣的雷火（指火炮）响彻天际吧！那些愚蠢的敌军，终将在皇女的威光下化为劫灰！」\n奥兹：「小姐觉得您的炮兵方阵很有气势。我会协助校准弹道，减少误伤和损耗。」",
          "modifier": {
            "army_artillery_power": 0.25,
            "land_unit_attrition": -0.2
          }
        },
        "transcended": {
          "key": "gacha_fischl_transcended_trait",
          "name": "净土的降临者",
          "desc": "菲谢尔：「哪怕身处异邦，皇女的高贵亦无法掩盖。看着吧，此地的臣民终将臣服于幽夜的法则，为我的降临献上赞歌！」\n奥兹：「小姐会努力学习这里的礼仪。不过……她这种自信的气场，似乎意外地很能镇住场面呢。」",
          "modifier": {
            "monthly_legitimacy": 0.08,
            "monthly_prestige": 0.08
          }
        },
        "c2": {
          "key": "gacha_fischl_c2_trait",
          "name": "极夜的真视界",
          "desc": "菲谢尔：「看啊，那伪善的盟约上缠绕着背叛的黑气！这群凡人的把戏，在本皇女眼中如同小丑的闹剧。」\n奥兹：「小姐发现那个邻国的大使在撒谎。另外，我们在他们的宫廷里安插了眼线……小姐说那是『命运的伏笔』。」",
          "aura_country_modifier": "gacha_fischl_c2_country_modifier",
          "modifier": {
            "gacha_country_aura": "yes",
            "spy_network_construction": 0.8,
            "max_diplomats": 2,
            "country_cabinet_efficiency": 0.1
          }
        },
        "c4": {
          "key": "gacha_fischl_c4_trait",
          "name": "皇女的幻梦庭",
          "desc": "菲谢尔：「这就是『皇女的幻梦』！现实的边界已然破碎，让敌人在无尽的迷宫中哀嚎，被漆黑的箭雨（火炮）吞噬吧！」\n奥兹：「小姐利用地形布置了伏击圈。在这片战场上，她编写的剧本……似乎真的变成了现实。」",
          "modifier": {
            "army_artillery_power": 0.5,
            "siege_ability": 0.2,
            "land_unit_attrition": -0.4,
            "global_hostile_attrition": 0.1
          }
        },
        "c6": {
          "key": "gacha_fischl_c6_trait",
          "name": "永夜之主",
          "desc": "菲谢尔：「不必再寻找归途了，奥兹。我看清楚了，这片大地……这片在此刻属于我们的疆域，正是新的『幽夜净土』。而你……（看向玩家）即是与我共赏这永恒黑夜的『祝圣之王』。」\n奥兹：「……小姐这次没有让我翻译。我想，您听懂了。」",
          "modifier": {
            "monthly_legitimacy": 0.16,
            "monthly_prestige": 0.16,
            "monthly_war_exhaustion": -0.1,
            "global_hostile_attrition": 0.2
          }
        }
      },
      "constellations": {
        "c0": {
          "name": "幻夜座·零命",
          "desc": "菲谢尔：「命运将我投放至此，必有其深意。」\n奥兹：「小姐的意思是：我们会成为您情报与侦察的眼睛。」"
        },
        "c1": {
          "name": "渡鸦的信使",
          "desc": "菲谢尔：「黑鸦振翼，便是密信抵达之时。」\n奥兹：「我会替您在各国宫廷间传递与截获消息。只要有窗与阴影，就有路。」"
        },
        "c2": {
          "name": "极夜之瞳",
          "desc": "菲谢尔：「谎言的颜色，逃不过极夜的瞳。」\n奥兹：「对方的使节在撒谎——我已经记下了他们真正想要的东西。」"
        },
        "c3": {
          "name": "皇女夜谭",
          "desc": "菲谢尔：「让史官写下吧：幽夜的篇章从此翻页。」\n奥兹：「小姐的“故事”会被当作军令与国策解读……效果意外地不错。"
        },
        "c4": {
          "name": "幻梦侵蚀",
          "desc": "菲谢尔：「在梦与现实的夹缝里，敌军将迷失方向。」\n奥兹：「她布置的佯动与伏击让敌人判断失灵；他们会把失败归咎于“诅咒”。」"
        },
        "c5": {
          "name": "宿命的仇敌",
          "desc": "菲谢尔：「命运指定的仇敌已现身——不必怜悯。」\n奥兹：「当我们锁定一个对手，小姐会异常专注。对方的每一次失误都会被放大。」"
        },
        "c6": {
          "name": "永夜净土",
          "desc": "菲谢尔：「此地既为净土，便当有永夜的法则。」\n奥兹：「她不再只是扮演皇女——她开始把“幽夜”变成国家的气质。」"
        }
      },
