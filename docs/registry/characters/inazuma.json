    {
      "code": "raiden",
      "name": {
        "first": "将军",
        "last": "雷电"
      },
      "traits": {
        "origin": {
          "key": "gacha_raiden_origin_trait",
          "name": "鸣神岛·永恒静土",
          "desc": "「所谓的『叛乱』，不过是杂音。只要威光足够耀眼，影子便无处遁形。我会让你的国家像静止的水面一样平稳。安静，是永恒的前提。」",
          "aura_country_modifier": "gacha_raiden_country_modifier",
          "modifier": {
            "gacha_country_aura": "yes",
            "monthly_war_exhaustion": -0.08,
            "monthly_legitimacy": 0.1,
            "discipline": 0.04,
            "local_unrest": -0.4
          }
        },
        "awakened": {
          "key": "gacha_raiden_awakened_trait",
          "name": "无想之一刀",
          "desc": "「这一刀，曾斩杀魔神，如今用来为你开疆拓土，倒也不算辱没。无需战术，无需诡计，只要我的雷光亮起，敌阵便已是灰烬。」",
          "modifier": {
            "discipline": 0.1,
            "army_initiative": 0.25
          }
        },
        "transcended": {
          "key": "gacha_raiden_transcended_trait",
          "name": "天下人之大权",
          "desc": "「权柄不容分割，意志不容违逆。既然让我坐在这个位置，我就不会允许任何『议会』或『阶层』的聒噪。此身即是绝对，此身即是法度。」",
          "modifier": {
            "country_cabinet_efficiency": 0.25,
            "monthly_war_exhaustion": -0.5
          }
        },
        "c2": {
          "key": "gacha_raiden_c2_trait",
          "name": "祸津·稻光",
          "desc": "「那些被征服的土地还在怀念旧主？可笑。我会用稻光斩断他们对过去的『妄念』。从今往后，他们的历史只有『现在』，他们的未来只有『你』。」",
          "aura_country_modifier": "gacha_raiden_c2_country_modifier",
          "modifier": {
            "gacha_country_aura": "yes",
            "siege_ability": 0.2,
            "artillery_bonus_vs_fort": 0.15,
            "land_morale": 0.1,
            "discipline": 0.05
          }
        },
        "c4": {
          "key": "gacha_raiden_c4_trait",
          "name": "梦想·真说",
          "desc": "「看着战士们冲锋的样子……我想起了曾经的笹百合与千代。……不，将军不会感伤。我将赐予全军『梦想一心』的加护。只要他们向我献上忠诚，我便许诺他们——必胜。」",
          "modifier": {
            "discipline": 0.2,
            "army_initiative": 0.5,
            "land_morale_recovery": 0.1
          }
        },
        "c6": {
          "key": "gacha_raiden_c6_trait",
          "name": "源流·许愿",
