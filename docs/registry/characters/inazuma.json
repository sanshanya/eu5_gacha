  "characters": [
    {
      "code": "ayaka",
      "name": {
        "first": "神里绫华",
        "last": "白鹭"
      },
      "traits": {
        "origin": {
          "key": "gacha_ayaka_origin_trait",
          "name": "白鹭姬的礼法",
          "desc": "「在剑拔弩张的宫廷里，礼法不是装饰，而是刀鞘。\n\n把尖锐的话包进得体的措辞里，把各阶层的情绪安放在可接受的秩序中——这就是社奉行的工作。\n\n若您愿意给我一间能安静落笔的书房，我会让您的朝会少一点争吵，多一点服从。」",
          "aura_country_modifier": "gacha_ayaka_country_modifier",
          "modifier": {
            "gacha_country_aura": "yes",
            "stability_cost": -0.08,
            "global_estate_satisfaction_recovery": 0.04,
            "send_diplomat_cost_modifier": -0.05,
            "legislative_efficiency": 0.05
          }
        },
        "awakened": {
          "key": "gacha_ayaka_awakened_trait",
          "name": "神里流·霜刃裁断",
          "desc": "「战场并不粗鄙，它只是更诚实。\n\n当谈判桌上仍有人试图用谎言拖延时，我会用一场干净利落的胜利，让他们重新学会‘守约’。\n\n请下令吧——我会让敌军明白，‘礼’与‘兵’，从来不是两条路。」",
          "modifier": {
            "land_morale_recovery": 0.04,
            "land_morale": 0.04,
            "land_unit_attrition": -0.025,
            "army_maintenance_cost": -0.025
          }
        },
        "transcended": {
          "key": "gacha_ayaka_transcended_trait",
          "name": "社奉行的治世之道",
          "desc": "「我见过太多国家死于‘正确’，也见过太多国家活于‘折中’。\n\n传统需要被尊重，但不该被供奉；改革需要被推动，但不该被崇拜。\n\n若您愿意把国是交给我分担，我会用最温和的方式推进最坚决的改变——让人们甚至来不及反对。」",
          "modifier": {
            "legislative_efficiency": 0.06,
            "country_cabinet_efficiency": 0.04,
            "stability_cost": -0.04,
            "global_estate_target_satisfaction": 0.025,
            "monthly_legitimacy": 0.025
          }
        },
        "c2": {
          "key": "gacha_ayaka_c2_trait",
          "name": "三奉行的棋局",
          "desc": "「人们以为权力来自武力，其实权力来自‘共识’。\n\n让武家安心，让商人得利，让神社有体面——三方都满意时，国家就不会被内耗拖住脚步。\n\n我不保证人人喜欢您，但我能保证：他们都会选择‘继续合作’。」",
          "aura_country_modifier": "gacha_ayaka_c2_country_modifier",
          "modifier": {
            "gacha_country_aura": "yes",
            "global_estate_target_satisfaction": 0.06,
            "stability_cost": -0.05,
            "global_defensive": 0.05
          }
        },
        "c4": {
          "key": "gacha_ayaka_c4_trait",
          "name": "雪关扉下的誓约",
          "desc": "「最冷的不是风雪，是军心动摇的那一刻。\n\n我会把粮秣、军饷、抚恤与荣誉一条条写清，让士兵知道自己为何而战，回乡时又能留下些什么。\n\n当秩序仍在，恐惧就进不来。」",
          "modifier": {
            "land_morale_recovery": 0.08,
            "land_morale": 0.08,
            "land_unit_attrition": -0.05,
            "army_maintenance_cost": -0.05
          }
        },
        "c6": {
          "key": "gacha_ayaka_c6_trait",
          "name": "白鹭之章·国是",
          "desc": "「我曾以为，‘白鹭姬’只是一种身份。\n\n但来到这里以后我才明白，它也可以是一种选择：在刀锋旁仍坚持温柔，在胜利后仍为失败者留路。\n\n陛下，若您要的是一个能让国家‘更像国家’的人——我愿意一直站在您身边。」",
          "modifier": {
            "legislative_efficiency": 0.12,
            "country_cabinet_efficiency": 0.08,
            "stability_cost": -0.08,
            "global_estate_target_satisfaction": 0.05,
            "monthly_legitimacy": 0.05
          }
        }
      },
      "constellations": {
        "c0": {
          "name": "白鹭座·零命",
          "desc": "社奉行的礼法被带入宫廷：谈判更顺滑，阶层更容易被安抚，仪式感变成可计算的秩序。"
        },
        "c1": {
          "name": "霜杀墨染樱",
          "desc": "每一次“温柔的提醒”背后都藏着锋利的底线：对外更难被试探底牌，对内更快压住流言与躁动。"
        },
        "c2": {
          "name": "三重雪关扉",
          "desc": "把防线写进制度：关隘、要塞与港口的调度更严密，敌人想钻空子会发现门后还有第二道门。"
        },
        "c3": {
          "name": "花白锦画纸吹雪",
          "desc": "以文化塑造合法性：节庆、礼乐与赞助让王权更“理所当然”，反对者会被舆论与风俗缓慢淹没。"
        },
        "c4": {
          "name": "盈缺流返",
          "desc": "危机时的回旋余地：当战事或饥荒逼近，朝廷更容易重新分配资源，避免秩序崩盘。"
        },
        "c5": {
          "name": "花云钟入月",
          "desc": "盟友愿意再多给你一次机会：外交场上更容易获得信任，甚至敌国也会更倾向先谈再打。"
        },
        "c6": {
          "name": "间水月",
          "desc": "决定性的一击不必喧嚣：关键会战中更容易打出“瞬间塌陷”的士气差，让胜利像雪落一样无声却彻底。"
        }
      },
