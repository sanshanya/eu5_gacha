    {
      "code": "kuki_shinobu",
      "name": {
        "first": "久岐忍",
        "last": "久岐"
      },
      "traits": {
        "origin": {
          "key": "gacha_kuki_shinobu_origin_trait",
          "name": "持证上岗",
          "desc": "「先说好：不按规章办事，我不接。\n\n账目要对，合同要签，惩戒要写进条款——这样才不会把国家治理变成‘看谁嗓门大’。\n\n你缺的是能干活的人，不是会表态的人。把麻烦交给我，我会把它处理成流程。」",
          "aura_country_modifier": "gacha_kuki_shinobu_country_modifier",
          "modifier": {
            "gacha_country_aura": "yes",
            "legislative_efficiency": 0.06,
            "stability_cost": -0.06,
            "tax_income_efficiency": 0.05,
            "local_unrest": -0.2,
            "counter_espionage": 0.05
          }
        },
        "awakened": {
          "key": "gacha_kuki_shinobu_awakened_trait",
          "name": "鬼之副手·整肃",
          "desc": "「市井的乱子，往往比战场更难。\n\n打仗靠火炮，治安靠执行。该抓的抓，该罚的罚——但每一笔都要有凭有据。\n\n你要的是‘可持续的威慑’，不是‘一时的恐吓’。」",
          "modifier": {
            "army_maintenance_cost": -0.04,
            "land_unit_attrition": -0.03,
            "land_morale_recovery": 0.03,
            "global_manpower_modifier": 0.025
          }
        },
        "transcended": {
          "key": "gacha_kuki_shinobu_transcended_trait",
          "name": "律法与刀刃",
          "desc": "「我在璃月学过律法，也在稻妻见过奉行的手段。\n\n真正有效的统治，是让人知道：违法不划算，守法更省事。\n\n如果你愿意让我把制度落到每个郡县，我能让腐败和走私像失血一样慢慢停下来。」",
          "modifier": {
            "legislative_efficiency": 0.07,
            "country_cabinet_efficiency": 0.05,
            "counter_espionage": 0.125,
            "stability_cost": -0.05,
            "local_unrest": -0.15
          }
        },
        "c2": {
          "key": "gacha_kuki_shinobu_c2_trait",
          "name": "割舍侥幸之心",
          "desc": "「靠‘运气’治理的国家，迟早会撞上必然的灾难。\n\n我会把风险拆开：粮、税、兵、路——每一条都做冗余。\n\n不一定最耀眼，但一定更不容易翻车。」",
          "aura_country_modifier": "gacha_kuki_shinobu_c2_country_modifier",
          "modifier": {
            "gacha_country_aura": "yes",
            "monthly_war_exhaustion": -0.03,
            "global_devastation_recovery": 0.15,
            "land_unit_attrition": -0.03
          }
        },
        "c4": {
          "key": "gacha_kuki_shinobu_c4_trait",
          "name": "越祓结界·后勤轮转",
          "desc": "「前线需要的不是口号，是补给。\n\n我会让军需像钟表一样转起来：仓储、转运、医护、抚恤，全部有人负责，全部能追责。\n\n你只管决定打不打，剩下的我来保证‘打得动’。」",
          "modifier": {
            "army_maintenance_cost": -0.08,
            "land_unit_attrition": -0.06,
            "land_morale_recovery": 0.06,
            "global_manpower_modifier": 0.05
          }
        },
        "c6": {
          "key": "gacha_kuki_shinobu_c6_trait",
          "name": "割舍软弱之心",
          "desc": "「我不是英雄。\n\n但当局面要崩的时候，我会站在最先裂开的地方，把那道口子补上。\n\n只要制度还在运转，国家就不会因为一次失误而倒下。」",
          "modifier": {
            "legislative_efficiency": 0.14,
            "country_cabinet_efficiency": 0.1,
            "counter_espionage": 0.25,
            "stability_cost": -0.1,
            "local_unrest": -0.3
          }
        }
      },
      "constellations": {
        "c0": {
          "name": "烦恼刈座·零命",
          "desc": "把“麻烦”变成“事务”：地方治理与治安处置更可控，流程开始取代拍脑袋。"
        },
        "c1": {
          "name": "割舍怜悯之心",
          "desc": "不被情绪牵着走：执法与惩戒更稳定，避免因徇私或软弱导致的反复动荡。"
        },
        "c2": {
          "name": "割舍侥幸之心",
          "desc": "不把国家押在运气上：财政与后勤的断点减少，边境更不容易被突袭打穿。"
        },
        "c3": {
          "name": "割舍痛苦之心",
          "desc": "把痛苦当作成本核算：战损与抚恤更制度化，民心更难被长期战争拖垮。"
        },
        "c4": {
          "name": "割舍封闭之心",
          "desc": "信息不再堵在上层：基层反馈更快回流，能更早发现腐败、走私与叛乱苗头。"
        },
        "c5": {
          "name": "割舍逢迎之心",
          "desc": "不靠讨好换支持：对权贵与阶层的管理更“硬”，但也更持久、更少反噬。"
        },
        "c6": {
          "name": "割舍软弱之心",
          "desc": "在最差的局面也能稳住：关键危机时更难被一波带走，国家更有“翻盘窗口”。"
        }
      },
