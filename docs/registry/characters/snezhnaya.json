    {
      "code": "columbina",
      "name": {
        "first": "哥伦比娅",
        "last": "少女"
      },
      "traits": {
        "origin": {
          "name": "静谧的圣歌",
          "desc": "她在雪夜里轻声哼唱，像在祝福，也像在送别。\n大厅里的争吵逐渐停下，连火炉都像是放轻了噼啪声。\n「……别急。」她说，「审判会来到的。」",
          "key": "gacha_columbina_origin_trait",
          "modifier": {
            "gacha_country_aura": "yes",
            "legislative_efficiency": 0.1
          },
          "aura_country_modifier": "gacha_columbina_country_modifier"
        },
        "awakened": {
          "name": "月下低语",
          "desc": "她不争辩，只把手指按在地图的几处：港口、修道院、粮仓。\n几周后，那些地方的谣言自己消失了，反对的声音也变得稀薄。\n你问她做了什么，她只是笑：「我把话送到该听的人耳边。」",
          "key": "gacha_columbina_awakened_trait",
          "modifier": {
            "monthly_war_exhaustion": -0.06,
            "global_devastation_recovery": 0.125,
            "global_estate_satisfaction_recovery": 0.03
          }
        },
        "transcended": {
          "name": "无垢的注视",
          "desc": "当她坐上高位，臣子不敢抬头。\n他们害怕的不是刑罚，而是自己的心思被看穿。\n你的法令执行得前所未有地顺畅——仿佛全境都在同一节拍里呼吸。",
          "key": "gacha_columbina_transcended_trait",
          "modifier": {
            "stability_cost": -0.09,
            "monthly_war_exhaustion": -0.08,
            "country_cabinet_efficiency": 0.06,
            "monthly_prestige": 0.06,
            "monthly_legitimacy": 0.06,
            "global_devastation_recovery": 0.175
          }
        },
        "c2": {
          "name": "安魂回环",
          "desc": "「恐惧会制造暴民，暴民会制造更大的恐惧。」\n她让教堂的钟声、集市的歌谣、军营的号角统一节奏。\n民心像被抚平的水面，动荡的火种不再易燃。",
          "key": "gacha_columbina_c2_trait",
          "modifier": {
            "gacha_country_aura": "yes",
            "legislative_efficiency": 0.2
          },
          "aura_country_modifier": "gacha_columbina_c2_country_modifier"
        },
        "c4": {
          "name": "无声的合唱",
          "desc": "她把彼此敌对的派系请进同一间厅堂，像排练一样让他们轮流发言。\n没有人被羞辱，也没有人被纵容。\n离开时，他们忽然学会了用同一种语气谈“国家”。",
          "key": "gacha_columbina_c4_trait",
          "modifier": {
            "monthly_war_exhaustion": -0.12,
            "global_devastation_recovery": 0.25,
            "global_estate_satisfaction_recovery": 0.06
          }
        },
        "c6": {
          "name": "归乡之梦",
          "desc": "「我并不渴望王座。」\n她轻声说，「我只是想看到，风暴之后还有人能回家。」\n在她的歌声里，你的国土更不易崩裂：疲惫会被安抚，裂痕会被缝合。",
          "key": "gacha_columbina_c6_trait",
          "modifier": {
            "stability_cost": -0.18,
            "monthly_war_exhaustion": -0.16,
            "country_cabinet_efficiency": 0.12,
            "monthly_prestige": 0.12,
            "monthly_legitimacy": 0.12,
            "global_devastation_recovery": 0.35
          }
        }
      },
      "constellations": {
        "c0": {
          "name": "空月座·零命",
          "desc": "解锁「少女」的治国风味：以“静默的影响”稳定宫廷与民心。（命之座为自拟占位）"
        },
        "c1": {
          "name": "无声的聆听",
          "desc": "宫廷阴谋更难扩散，流言造成的动荡更易被压住。"
        },
        "c2": {
          "name": "月下低语",
          "desc": "对叛乱与分裂的敏感度提升，提前发现并拆解风险。"
        },
        "c3": {
          "name": "白羽的誓言",
          "desc": "外交场景更易占据道德高地，谈判阻力降低。"
        },
        "c4": {
          "name": "安魂回环",
          "desc": "战争疲惫与社会撕裂的累积更慢，长期战争更可控。"
        },
        "c5": {
          "name": "静海之梦",
          "desc": "在危机与灾荒时仍能维持秩序与生产，减少连锁崩坏。"
        },
        "c6": {
          "name": "归乡的终曲",
          "desc": "当国家濒临失序时触发强力“稳定窗口”，为重整赢得时间。"
        }
      },
